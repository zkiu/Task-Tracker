{
	"functions": {
		"predeploy": ["npm --prefix \"$RESOURCE_DIR\" run lint"],
		"source": "functions"
	},
	"hosting": {
		"public": "build",
		"headers": [
			{
				"source": "/service-worker.js",
				"headers": [{"key": "Cache-Control", "value": "no-cache"}]
			}
		],
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "**",
				"destination": "/index.html"
			}
		]
	}
}
